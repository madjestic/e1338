name:          e1338
version:       0.2.0.2
synopsis:      A re-write of e1337 with BearRiver (and, maybe, MVars)
description:   Please see the README on GitHub at <https://github.com/madjestic/e1338#readme>
homepage:      https://github.com/madjestic/e1338
license:       BSD3
license-file:  LICENSE
author:        madjestic
maintainer:    madjestic13@gmail.com
copyright:     Vladimir Lopatin
category:      App
build-type:    Simple
cabal-version: >=2.0

extra-source-files:
                ChangeLog.md
                README.md

source-repository head
  type:         git
  location:     https://github.com/madjestic/e1338

library
  hs-source-dirs:
                src
  build-depends:
                base                ^>= 4.14
              , aeson               ^>= 1.4.7.1
              , aeson-pretty        ^>= 0.8.9
              , bearriver           ^>= 0.13.1.2
              , bytestring          ^>= 0.10.12.1
              , containers          ^>= 0.6.5.1
              , GLUtil              ^>= 0.10.3
              , ilist               ^>= 0.2.0.0
              , lens                ^>= 4.19.2
              , linear              ^>= 1.20.9
              , locators            ^>= 0.3.0.3              
              , massiv              ^>= 1.0.1.1
              , OpenGL              ^>= 3.0.3.0
              , random              ^>= 1.2.1              
              , sdl2                ^>= 2.5.3.0
              , simple-affine-space ^>= 0.1.1
              , sort                ^>= 1.0.0.0
              , split               ^>= 0.2.3.4
              , store               ^>= 0.7.14
              , text                ^>= 1.2.5.0
              --, Yampa               ^>= 0.13.1
              , vector              ^>= 0.12.1.2
              , uuid                ^>= 1.3.15
  
  exposed-modules:
                AppInput
              , Camera
              , Controllable
              , Descriptor
              , Application
              , App
              , Keyboard
              , Material
              , Model
              , Object
              , PGeo
              , Project
              , Rendering
              , Solvable
              , Texture
              , Utils
              , VAO
              , Update
              , Graph              
              
  other-modules:
                Paths_e1338
              , LoadShaders
              , FromVector
              , Mouse
  default-language:
                Haskell2010

