                     , aeson               ^>= 1.4.7.1
                     , aeson-pretty        ^>= 0.8.9                     
                     , bearriver           ^>= 0.13.1.2
                     , bytestring          ^>= 0.10.12.1
                     , containers          ^>= 0.6.5.1                     
                     , ilist               ^>= 0.2.0.0                     
                     , lens                ^>= 4.19.2
                     , linear              ^>= 1.20.9
                     , locators            ^>= 0.3.0.3                     
                     , massiv              ^>= 1.0.1.1
                     , random              ^>= 1.2.1
                     , vector
                     , GLUT
                     , GLUtil
                     , OpenGL
                     , sdl2
                     , simple-affine-space ^>= 0.1.1                     
                     , splitmix
                     , split               ^>= 0.2.3.4                     
                     , sort                ^>= 1.0.0.0                     
                     , store               ^>= 0.7.14                     
                     , text                ^>= 1.2.5.0
                     , uuid                ^>= 1.3.15
  other-modules:
                Paths_e1338
              , App
              , AppInput
              , Application
              , Camera
              , Controllable
              , Descriptor
              , FromVector
              , Graph
              , Keyboard
              , LoadShaders
              , Material
              , Model
              , Mouse
              , Object
              , PGeo
              , Project
              , Rendering
              , Solvable
              , Texture
              , Utils
              , VAO
              , HMap
              , Update
              , Graph              

  default-language:    Haskell2010
                
                                                                           
